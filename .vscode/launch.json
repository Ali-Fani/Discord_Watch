{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "debugpy",
      "request": "launch", 
      "name": "Launch Discord Bot",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "type": "debugpy",
      "request": "launch",
      "name": "Launch Health API",
      "program": "${workspaceFolder}/health.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    }
  ]
}